import org.caleb.movieclip.CoreMovieClip;
import org.caleb.collection.AbstractList
import com.sephiroth.XML2Object;

class org.caleb.components.ticker.TickerDisplayData extends CoreMovieClip
{
	private var $iconUrl:String;
	private var $title:String;
	private var $xml:XML;
	private var $displayItems:Array;
	private var $displayIcons:Array;
	private var $displayTitles:Array;
	
	function TickerDisplayData()
	{
		this.setClassDescription('org.caleb.components.ticker.TickerDisplayData');
		this.$displayItems = new Array();
		this.$displayIcons = new Array();
		this.$displayTitles = new Array();
	}
	// read only
	public function get displayItems():Array
	{
		return this.$displayItems;
	}
	public function get displayIcons():Array
	{
		return this.$displayIcons;
	}
	public function get displayTitles():Array
	{
		return this.$displayTitles;
	}
}