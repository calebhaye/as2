class org.caleb.components.menu.XMLMenuActions extends Object
{
	public function XMLMenuActions()
	{
	}
	public function gotoURL(urlVar)
	{
		getURL(urlVar, "_blank");
	}
	public function message(msg)
	{
		trace(msg);
		// message_txt.text = msg;
	}
	public function newMenu(menu:org.caleb.components.menu.impl.CascadingXMLMenu, xml:String)
	{
		menu.init(xml);
	}
}