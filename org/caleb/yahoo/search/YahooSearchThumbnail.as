class org.caleb.yahoo.search.YahooSearchThumbnail extends Object
{
	private var $Url:String;
	private var $Height:Number;
	private var $Width:Number;

	/*
	 * Sole Constructor 
	 */
	public function YahooSearchThumbnail(Url:String, Height:Number, Width:Number)
	{
		this.$Url = Url;
		this.$Height = Height;
		this.$Width = Width;
	}
	/*
	 * Accessor for private $Url var
	 */
	public function get Url():String
	{
		return this.$Url;
	}
	/*
	 * Accessor for private $Height var
	 */
	public function get Height():Number
	{
		return this.$Height;
	}
	/*
	 * Accessor for private $Width var
	 */
	public function get Width():Number
	{
		return this.$Width;
	}
}